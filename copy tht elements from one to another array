#include <stdio.h>
#define MAX_SIZE 100
void main() 
{
int arr1[MAX_SIZE], arr2[MAX_SIZE];
int size,i;
printf("Enter the size of the array: ");
scanf("%d",&size);
printf("Enter elements of array:\n");
for (i = 0; i < size; i++) 
{
scanf("%d", &arr1[i]);
}
for (i = 0; i < size; i++) 
{
arr2[i] = arr1[i];
}
printf("\nElements of original array: ");
for (i = 0; i < size; i++) 
{
printf("%d ", arr1[i]);
}
printf("\nElements of new array: ");
for (i = 0; i < size; i++) {
printf("%d ", arr2[i]);
}
printf("\n");
}
